import pandas as pd
import numpy as np

# 1. Buat angka urut 0-255
data = np.arange(256)

# 2. Acak posisinya (Shuffle) agar menjadi S-box yang unik
np.random.shuffle(data)

# 3. Ubah menjadi bentuk Matrix 16x16
matrix = data.reshape((16, 16))

# 4. Simpan ke Excel tanpa Header dan Index
df = pd.DataFrame(matrix)
df.to_excel("random_sbox.xlsx", index=False, header=False)

print("Berhasil! File 'random_sbox.xlsx' siap digunakan untuk tes.")